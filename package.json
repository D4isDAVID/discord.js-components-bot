{
  "name": "discord.js-components-bot",
  "description": "Discord.js bot template with components system",
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "lint": "prettier --ignore-path .gitignore -c .",
    "format": "prettier --ignore-path .gitignore -w .",
    "build": "tsc",
    "test": "npm run build && npm run lint",
    "deploy": "npm run build && node dist/deploy.js",
    "start": "npm run build && node ."
  },
  "dependencies": {
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "prettier": "^2.8.4"
  },
  "devDependencies": {
    "@types/node": "^18.15.0",
    "typescript": "^4.9.5"
  }
}
